<#@ template inherits="ETemplate" language="VB" #>
<# Dim JExt as JExtension = CType(Ext, JExtension) #>
<?xml version="1.0" encoding="utf-8"?>
<metadata>
	<layout title="<#= JExt.GenText(Value("Task.nameObject") & " view default title", StrConv(Value("Task.nameObject") & " Form", VbStrConv.Propercase)) #>"
	        option="<#= JExt.GenText(Value("Task.nameObject") & " view default option", "Default") #>">
		<message>
			<![CDATA[<#= JExt.GenText(Value("Task.nameObject") & " view default desc", Value("Task.message")) #>]]>
		</message>
	</layout>
</metadata>

