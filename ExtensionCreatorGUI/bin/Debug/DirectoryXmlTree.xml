            <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\components\com_listing\views\listing" ArchivePath="\site\views\listing" addFile="IndexHtmlFile" langFile="site">index.html</file>
          </folder>
          <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\components\com_listing\views" ArchivePath="\site\views" addFile="IndexHtmlFile" langFile="site">index.html</file>
        </folder>
        <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\components\com_listing" ArchivePath="\site" addFile="IndexHtmlFile" langFile="site">index.html</file>
      </folder>
