<?xml version="1.0" encoding="utf-8"?>
<templateFiles>
  <files for="Language" id="Language1">
    <folder name="language" exportAs="language" archiveAs="site" FilePath="\language" ArchivePath="\site">
      <folder archiveAs="site\language" name="en-GB" FilePath="\language\en-GB" ArchivePath="\site\language">
        <file type="site" template="langFile.ini" FilePath="\language\en-GB" ArchivePath="\site\language">en-GB.com_listing.ini</file>
      </folder>
    </folder>
    <folder name="administrator" exportAs="administrator" FilePath="\administrator">
      <folder archiveAs="admin" name="language" FilePath="\administrator\language" ArchivePath="\admin">
        <folder archiveAs="admin\language" name="en-GB" FilePath="\administrator\language\en-GB" ArchivePath="\admin\language">
          <file type="admin" template="langFile.ini" FilePath="\administrator\language\en-GB" ArchivePath="\admin\language">en-GB.com_listing.ini</file>
          <file type="admin.sys" template="langFile.ini" FilePath="\administrator\language\en-GB" ArchivePath="\admin\language">en-GB.com_listing.sys.ini</file>
        </folder>
      </folder>
    </folder>
  </files>
  <files for="Main" id="Main1">
    <!-- The Main files are all necessary, don't remove any of them. -->
    <folder name="components" langFile="site" exportAs="components" FilePath="\components">
      <folder name="com_listing" readInNodes="true" addFile="IndexHtmlFile" archiveAs="site" listContentsTo="/extension/files[@folder='site']" FilePath="\components\com_listing" ArchivePath="\site" langFile="site">
        <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\components\com_listing" ArchivePath="\site" readInNodes="true" addFile="IndexHtmlFile" langFile="site">index.html</file>
        <folder processFile="false" name="models"></folder>
        <folder processFile="false" name="views"></folder>
        <file processFile="false">controller.php</file>
        <file processFile="false">listing.php</file>
      </folder>
    </folder>
    <folder name="administrator" langFile="admin" exportAs="administrator" FilePath="\administrator">
      <folder name="components" FilePath="\administrator\components" langFile="admin">
        <folder name="com_listing" readInNodes="true" addFile="IndexHtmlFile" archiveAs="admin" listContentsTo="/extension/administration/files[@folder='admin']" formFile="config" FilePath="\administrator\components\com_listing" ArchivePath="\admin" langFile="admin">
          <!-- Make sure the intall file is loaded first, so its parameters can be used. -->
          <!-- For the install file, only need the install attribute to be there (value doesn't matter). -->
          <file template="install.xml" archiveAs="" langFile="admin.sys" install="true" FilePath="\administrator\components\com_listing" ArchivePath="" readInNodes="true" addFile="IndexHtmlFile" formFile="config">listing.xml</file>
          <!-- Make sure this file appears before files that may add parameters to it. -->
          <file template="beConfig.xml" langFile="admin" openJform="true" formTitle="Configurations form" formSaveMethod="4" hasForm="config" formFieldset="main" FilePath="\administrator\components\com_listing" ArchivePath="\admin" readInNodes="true" addFile="IndexHtmlFile" formFile="config">config.xml</file>
          <file template="beInstallScript.php" archiveAs="" langFile="admin.sys" listContentsTo="false" FilePath="\administrator\components\com_listing" ArchivePath="" readInNodes="true" addFile="IndexHtmlFile" formFile="config">script.php</file>
          <folder name="assets" FilePath="\administrator\components\com_listing\assets" ArchivePath="\admin\assets" readInNodes="true" addFile="IndexHtmlFile" formFile="config" langFile="admin">
            <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\administrator\components\com_listing\assets" ArchivePath="\admin\assets" readInNodes="true" addFile="IndexHtmlFile" formFile="config" langFile="admin">index.html</file>
          </folder>
          <file template="beAccess.xml" langFile="admin.sys" FilePath="\administrator\components\com_listing" ArchivePath="\admin" readInNodes="true" addFile="IndexHtmlFile" formFile="config">access.xml</file>
          <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\administrator\components\com_listing" ArchivePath="\admin" readInNodes="true" addFile="IndexHtmlFile" formFile="config" langFile="admin">index.html</file>
        </folder>
      </folder>
    </folder>
    <folder name="media" exportAs="media" FilePath="\media">
      <folder name="com_listing" archiveAs="media" readInNodes="true" addFile="IndexHtmlFile" listContentsTo="/extension/media" FilePath="\media\com_listing" ArchivePath="\media">
        <folder name="css" FilePath="\media\com_listing\css" ArchivePath="\media\css" readInNodes="true" addFile="IndexHtmlFile">
          <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\media\com_listing\css" ArchivePath="\media\css" readInNodes="true" addFile="IndexHtmlFile">index.html</file>
        </folder>
        <folder name="images" FilePath="\media\com_listing\images" ArchivePath="\media\images" readInNodes="true" addFile="IndexHtmlFile">
          <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\media\com_listing\images" ArchivePath="\media\images" readInNodes="true" addFile="IndexHtmlFile">index.html</file>
        </folder>
        <folder name="js" FilePath="\media\com_listing\js" ArchivePath="\media\js" readInNodes="true" addFile="IndexHtmlFile">
          <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\media\com_listing\js" ArchivePath="\media\js" readInNodes="true" addFile="IndexHtmlFile">index.html</file>
        </folder>
        <file name="IndexHtmlFile" processFile="true" function="CreateIndexHtmlFile" FilePath="\media\com_listing" ArchivePath="\media" readInNodes="true" addFile="IndexHtmlFile">index.html</file>
      </folder>
    </folder>
  </files>
