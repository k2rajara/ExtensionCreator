<?xml version="1.0" encoding="UTF-8"?>
<extension>
	<parameters>
		<loadTemplate from="../config/profile.xml" />
		<extensionWide>
			<joomlaVersion>1.6.0</joomlaVersion>
			<!--  The version string is recorded in the components table -->
			<!--  This is the default version for new extensions. -->
			<version>1.0.0</version>
			<intallMethod>upgrade</intallMethod>
			<!--  Toggles showing JForm xml files for existing extensions. -->
			<showFormForExistingExtension>false</showFormForExistingExtension>
		</extensionWide>
		<files>
			<!-- Root directory for output files. -->
			<!-- C:\Users\Home\Documents\Job\workspace\Joomla 1.6 Work\test -->
			<!-- relUri="true">..\..\Output</OutputFolder -->
			<!-- C:\Users\Home\Documents\Job\workspace\Infinity Health Centre -->
			<OutputFolder>C:\xampp\apps\joomla\htdocs</OutputFolder>
			<ArchiveDirectory relUri="true">..\..\Archives</ArchiveDirectory>
			<exportFileName>export.zip</exportFileName>
		</files>
	</parameters>
	<taskDefinitions>
		<taskDefinition name="Language" >
			<parameters>
				<lang>en-GB</lang>
			</parameters>
		</taskDefinition>
	</taskDefinitions>
</extension>