<?xml version="1.0" encoding="utf-8"?>
<extension
	version="1.6.0"
	type="plugin"
	group="content">
	<name>PLG_CONTENT_MYQUESTION</name>
	<author>Krishan Rajaratnam</author>
	<creationDate>12/25/2017 00:00:00</creationDate>
	<copyright>Copyright (C) 2005 - 2016 Open Source Matters. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later</license>
	<authorEmail>kr.rajaratnam@mail.utoronto.ca</authorEmail>
	<authorUrl>http://krishanr.nfshost.com/</authorUrl>
	<version>1.0.0</version>
	<description></description>
	<files
		folder="myquestion">
		<filename
			plugin="myquestion">myquestion.php</filename>
		<filename>index.html</filename>
	</files>
	<!-- Media files -->
	<media
		destination="plg_myquestion"
		folder="media">
		<folder>css</folder>
		<folder>images</folder>
		<folder>js</folder>
		<filename>index.html</filename>
	</media>
	<languages
		folder="language">
		<language
			tag="en-GB">en-GB.plg_content_myquestion.ini</language>
		<language
			tag="en-GB">en-GB.plg_content_myquestion.sys.ini</language>
	</languages>
	<config>
		<fields
			name="params">
			<fieldset
				name="basic"
				label="Basic" />
			<fieldset
				name="advanced"
				label="Advanced" />
		</fields>
	</config>
</extension>