<?xml version="1.0" encoding="UTF-8"?>
<!-- This is a model for Joomla Libraries. -->
<library>
	<parameters>
		<loadTemplate from="extension.xml" select="extension/parameters/*" />
		<config>
			<templateClass>JExtension</templateClass>
			<assemblyFile>JoomlaLibrary.dll</assemblyFile>
			<extensionPrefix>lib</extensionPrefix>
			<extensionType>library</extensionType>
		</config>
		<!-- File system name of the library. -->
		<nameType>plain</nameType>
		<files>
			<!-- Root directory for template files. -->
			<TemplateFolder relUri="true">..\..\Templates\j16_library</TemplateFolder>
			<templateFiles internal="true" />
		</files>				
	</parameters>
	<taskDefinitions>
		<taskDefinition name="Main" />
		<loadTemplate from="extension.xml" select="extension/taskDefinitions/*" />
	</taskDefinitions>
</library>