<?xml version="1.0" encoding="UTF-8"?>
<!-- This is a template model for Joomla Modules. -->
<module>
	<parameters>
		<loadTemplate from="extension.xml" select="extension/parameters/*" />
		<config>
			<templateClass>JExtension</templateClass>
			<assemblyFile>JoomlaLibrary.dll</assemblyFile>
			<extensionPrefix>mod</extensionPrefix>
			<extensionType>module</extensionType>
		</config>
		<!-- Make sure the name is lower case, and seperate words with underscores (_).  -->
		<nameType>underscores</nameType>
		<files>
			<!-- Root directory for template files. -->
			<TemplateFolder relUri="true">..\..\Templates\j16_module</TemplateFolder>
			<templateFiles internal="true" />
		</files>
	</parameters>
	<taskDefinitions>
		<taskDefinition name="Main" />
		<loadTemplate from="extension.xml" select="extension/taskDefinitions/*" />
	</taskDefinitions>
</module>